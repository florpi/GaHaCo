{
    "label": "dark_or_light",
    "sampling": {"module": "imblearn.under_sampling",
                "method": "RandomUnderSampler"},
    "model": {
        "name": "catboost",
        "module": "catboost",
        "parameters": {
            "learning_rate": 0.05,
            "n_estimators": 50,
            "max_depth": 10,
            "colsample_bylevel": 1.0,

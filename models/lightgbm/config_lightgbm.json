{
    "label": "dark_or_light",
    "model": {
        "name": "lgbm",
        "module": "lightgbm",
        "class": "LGBMClassifier",
        "parameters": {
            "application": "binary",
            "objective": "binary",
            "metric": "auc",
            "is_unbalance": "true",
            "verbose": 0,
            "bagging_fraction": 0.05,
            "feature_fraction": 0.34,
            "learning_rate": 0.014,
            "max_depth":9,
            "min_split_gain": 0.035,
            "num_leaves": 76
        }
    },
    "metric": {
        "module": "sklearn.metrics",
        "method": "f1_score",
        "params": {"average":"binary"}
    },
    "feature_optimization":{
        "measure_importance": 1,
        "uncorrelated":1,
        "save_importance": 0
    }
}
